{
    "presets":[
        ["@babel/preset-env",{
            "targets": {
                "edge": "17",
                "firefox": "60",
                "chrome": "67",
                "safari": "11.1",
                "ie":"11"
              },
            "useBuiltIns": "usage",
            "corejs": { "version": 3, "proposals": true }
        }],
        "@babel/preset-react" 
    ], 
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
}